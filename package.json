{
  "name": "x-project",
  "version": "1.0.0",
  "description": "Discord Bot",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "start:dev": "tsc --watch & nodemon --ignore '*-base.js' dist/main.js",
    "lint": "prettier --cache --check . && eslint src --ext mjs,js,ts --cache",
    "format": "prettier --cache --write . && eslint src --ext mjs,js,ts --fix --cache",
    "build": "rimraf dist && tsc",
    "build:check": "tsc --noEmit --incremental false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlanPerdomo/x-project.git"
  },
  "keywords": [
    "Discord",
    "Bot",
    "NodeJS"
  ],
  "author": "Alan Perdomo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlanPerdomo/x-project/issues"
  },
  "homepage": "https://github.com/AlanPerdomo/x-project#readme",
  "dependencies": {
    "@discord-player/ffmpeg": "^0.1.0",
    "@discordjs/opus": "0.9.0",
    "@discordjs/voice": "^0.18.0",
    "@distube/ytdl-core": "^4.15.8",
    "@napi-rs/canvas": "0.1.63",
    "@ngrok/ngrok": "1.4.1",
    "axios": "1.7.7",
    "cheerio": "1.0.0",
    "discord.js": "^14.16.3",
    "dotenv": "16.4.5",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "2.1.3",
    "libsodium-wrappers": "0.7.15",
    "nodemon": "^3.1.9",
    "opusscript": "^0.0.8",
    "pg": "8.13.1",
    "prettier": "3.3.3",
    "prism-media": "^1.3.5",
    "sodium": "3.0.2",
    "sodium-native": "4.3.1",
    "tweetnacl": "1.0.3",
    "youtube-search-api": "^1.2.2"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "eslint": "9.15.0",
    "eslint-config-marine": "9.4.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "globals": "15.12.0",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "typescript": "5.7.2",
    "typescript-eslint": "8.15.0"
  },
  "resolutions": {
    "discord-api-types": "0.37.100"
  }
}
